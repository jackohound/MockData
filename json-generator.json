[
  {
    'repeat(10, 10)': {
      id: '{{objectId()}}',
      index: '{{index()}}',
      guid: '{{guid()}}',
      time: '{{moment(this.date(new Date(2021, 3, 20), new Date())).format("LLLL")}}',
      recurrance: '{{random("daily", "weekly", "bi-weekly")}}',
      endDate: '{{moment(this.date(new Date(), new Date(2023, 0, 1))).format("LLLL")}}',
      isActive: '{{bool()}}',
      photoUrl: '{{random("http://img.thevillages.com/Attachments/Unit/09-2017/Alden Bungalows.jpg", "http://img.thevillages.com/Attachments/Unit/05-2017/NRC%20-%20Amelia_1.jpg", "http://img.thevillages.com/Attachments/CommLease/01-2017/771%20-%20ABM.jpg", "http://img.thevillages.com/Attachments/Unit/09-2017/Antrim Dells_1.jpg")}}',
      photos: [
        {
          'repeat(3)': {
            id: '{{index()}}',
            photoUrl: 'http://placehold.it/32x32',
          }
       }
      ],
      webUrl: 'http://www.google.com',
      owner: {
        id: '{{objectId()}}',
        first: '{{firstName()}}',
        last: '{{surname()}}'
      },
      contact(tags) {
        return `${this.owner.first}.${this.owner.last}@${tags.company()}${tags.domainZone()}`.toLowerCase();
      },
      shortDesc: '{{lorem(6, "words")}}',
      longDesc: '{{lorem(1, "paragraphs")}}',
      keywords: [
        {
          'repeat(5)': '{{lorem(1, "words")}}'
        }
      ],
      city: '{{city()}}',
      state: '{{state()}}',
      address(tags) { 
        return `${tags.integer(100, 999)} ${tags.street()}, ${this.city}, ${this.state}, ${tags.integer(100, 10000)}`;
                    },
      latitude: '{{floating(-90.000001, 90)}}',
      longitude: '{{floating(-180.000001, 180)}}',
      attendees: [
        {
          'repeat(3)': {
            id: '{{index()}}',
            name: '{{firstName()}} {{surname()}}'
          }
        }
      ],
    }
  }
]